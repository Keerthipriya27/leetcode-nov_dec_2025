class Solution:
    def countPalindromicSubsequence(self, s: str) -> int:
        result = 0
        for c in 'abcdefghijklmnopqrstuvwxyz':
            first = s.find(c)
            last = s.rfind(c)
            if first != -1 and last != -1 and first < last:
                middle_chars = set(s[first+1:last])
                result += len(middle_chars)
        return result
