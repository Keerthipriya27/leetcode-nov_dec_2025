class Solution:
    def numberOfSubstrings(self, s: str) -> int:
        n=len(s)
        K=int(n**0.5)
        ans=0
        for i in range(n):
            ones=zeros=0
            for j in range(i,n):
                if s[j]=='1':
                    ones+=1
                else:
                    zeros+=1
                    if zeros>K:
                        break
                if ones>=zeros*zeros:
                    ans+=1
        return ans
