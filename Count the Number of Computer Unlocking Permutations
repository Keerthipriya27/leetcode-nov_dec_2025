class Solution:
    def countPermutations(self, complexity: List[int]) -> int:
        length = len(complexity)
        for idx in range(1, length):
            if complexity[idx] <= complexity[0]:
                return 0

        count, modulo = 1, 10**9 + 7
        for idx in range(2, length):
            count = count * idx % modulo
        return count
